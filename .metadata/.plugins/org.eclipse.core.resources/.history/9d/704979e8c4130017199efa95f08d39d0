package br.com.fiap.bean;

import javax.faces.bean.ManagedBean;

import br.com.fiap.repository.CompraRepository;
import br.com.fiap.to.CompraTO;

@ManagedBean
public class CompraBean {
	private CompraTO to;
	private CompraRepository repository = new CompraRepository();
	
	public void cadastrar(){
		try {
			repository.cadastrar(to);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}

	public CompraTO getTo() {
		return to;
	}

	public void setTo(CompraTO to) {
		this.to = to;
	}

	
}
