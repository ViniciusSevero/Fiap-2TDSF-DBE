	public static void main(String[] args) {
		try{
			CursoBOStub bo = new CursoBOStub();
			CalcularMedia valores = new CalcularMedia();
			bo.calcularMedia(valores);
		}catch(AxisFault e){
			e.printStackTrace();
		}

	}

}
